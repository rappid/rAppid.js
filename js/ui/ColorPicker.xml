<ui:ColorPickerClass xmlns="http://www.w3.org/1999/xhtml" xmlns:ui="js.ui" xmlns:js="js.core"
                    componentClass="color-picker">

    <div class="color-picker-palette"
         background-color="{_backgroundColor()}"
         width="{paletteWidth}"
         height="{paletteHeight}"
         position="relative"
         onpointerdown="_paletteMouseDown">

        <img onmousedown="_imageMouseDown" width="{paletteWidth}" height="{paletteHeight}" title="" alt="" cid="paletteImage"
             src="data:image/png;base64,"/>
        <div cid="circle" class="color-picker-circle" top="{_palettePosition.y}" left="{_palettePosition.x}" position="absolute">
            <div>
                <span/>
            </div>
        </div>
    </div>
    <div class="color-picker-hue" position="relative" onpointerdown="_hueBarDown">
        <!--<div class="color-picker-hue-bar-wrapper" >-->
            <img cid="hueBar"
                 onmousedown="_imageMouseDown"
                 class="color-picker-hue-bar" height="{paletteHeight}" width="10"
                 src="data:image/png;base64," alt="hueBar"/>
        <!--</div>-->
        <div class="color-picker-hue-slider" top="{_scale}">
            <div>
                <span/>
            </div>
        </div>
    </div>

</ui:ColorPickerClass>